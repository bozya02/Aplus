<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:Aplus.PagesProject"
            x:Class="Aplus.PageProject"
            Visual="Material">
    <TabbedPage.ToolbarItems>
        <ToolbarItem Priority="1" Order="Primary" Clicked="tbEditClicked">
            <ToolbarItem.IconImageSource>
                <FontImageSource FontFamily="MaterailIcons" Glyph="&#xe3c9;"/>
            </ToolbarItem.IconImageSource>
        </ToolbarItem>
    </TabbedPage.ToolbarItems>
</TabbedPage>